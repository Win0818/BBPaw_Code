<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     android:background="@drawable/backgroud"
    tools:context=".InstallApkActivity" >

    
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="@string/hello_world" />

    <TextView
        android:id="@+id/textView_chipid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="@dimen/textView_chipid_layout_marginLeft"
        android:layout_marginTop="@dimen/textView_chipid_layout_marginTop"
        android:textSize="@dimen/textView_chipid_textsize" />


</RelativeLayout>
